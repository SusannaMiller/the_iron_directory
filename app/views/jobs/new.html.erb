<%= semantic_form_for [@student, @job] do |f| %>
	<%= f.inputs do %>
		<%= f.input :company %>
		<%= f.input :title %>
		<%= f.input :start_month %>
		<%= f.input :start_year %>
		<%= f.input :end_month %>
		<%= f.input :end_year %>
		<%= f.input :responsibilities %>
	<% end %>
	<%= f.actions %>
<% end %>
