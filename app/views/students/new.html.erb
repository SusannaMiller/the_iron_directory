<%= semantic_form_for @student do |f| %>
	<%= f.inputs do %>
		<%= f.input :first_name %>
		<%= f.input :last_name %>
		<%= f.input :cohort %>
		<%= f.input :course %>
		<%= f.input :photo %>
	<% end %>
  <%- i = 0 %>
  <%= f.inputs for: :contacts do |c| %>
    <%= c.input :category, as: :hidden, input_html: {value: @cats[i]} %>
    <%= c.input :info, label: @cats[i] %>
    <%- i = i + 1 %>
  <% end %>
	<%= f.actions %>
<% end %>